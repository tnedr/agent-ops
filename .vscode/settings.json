{
  "command-runner.commands": {
    "math.multiply": "python ${workspaceFolder}/scripts/multiply.py ${input:a} ${input:b}",
    "time.now": "python ${workspaceFolder}/scripts/current_time.py",
    "env.check": "python ${workspaceFolder}/scripts/venv_check.py"
  },
  "command-runner.inputs": {
    "a": {
      "type": "promptString",
      "description": "Első tényező"
    },
    "b": {
      "type": "promptString",
      "description": "Második tényező"
    }
  }
}